{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "no-unused-vars": 1,
    "camelcase": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "no-return-await": 0,
    "arrow-body-style": 0,
    "object-shorthand": 0,
    "no-else-return": 0,
    "no-unused-expressions": 0
  },
  "env": {
    "browser": true
  }
}